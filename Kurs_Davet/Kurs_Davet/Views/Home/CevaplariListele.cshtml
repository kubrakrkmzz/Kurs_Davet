@model IEnumerable<Kurs_Davet.Models.Bilgiler>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Katılım Sağlayacaklar</title>
</head>
<body>

    <h2> Kursa Katılım Sağlayacakların Bilgileri</h2>
    <table>
        <thead>
            <tr>
                <th>Ad</th>
                <th>Soyad</th>
                <th>Email</th>
                <th>Telefon</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Kurs_Davet.Models.Bilgiler x in Model)
            {
                <tr>
                    <td>@x.Ad</td>
                    <td>@x.Soyad</td>
                    <td>@x.Email</td>
                    <td>@x.Telefon</td>
                </tr>
            }
        </tbody>





    </table>






</body>
</html>
